{
  "name": "lolmetamind-electron",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "build": {
    "appId": "com.lolmetamind.app"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "electron": "cross-env NODE_ENV='development' electron .",
    "electron:dev": "concurrently --kill-others \"npm run dev\" \"wait-on http://localhost:3000 && npm run electron\""
  },
  "dependencies": {
    "concurrently": "^8.2.2",
    "electron": "^22.3.27",
    "framer-motion": "^12.23.18",
    "next": "^14.2.32",
    "react": "^18",
    "react-dom": "^18",
    "react-draggable": "^4.4.6",
    "react-icons": "^5.5.0",
    "tailwindcss-textshadow": "^2.1.3",
    "wait-on": "^7.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.0.0",
    "electron-builder": "^23.6.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17"
  }
}
